<%= turbo_frame_tag "new_spreadsheet" do %>
  <%= link_to :create, new_spreadsheet_path %>
<% end %>

<ul class="w-48 text-sm font-medium border rounded-lg bg-gray-700 border-gray-600 text-white">
  <% @spreadsheets.each do |spreadsheet| %>
    <li class="w-full px-4 py-2 border-b border-gray-600">
      <%= spreadsheet.name %>
    </li>
  <% end %>
</ul>
